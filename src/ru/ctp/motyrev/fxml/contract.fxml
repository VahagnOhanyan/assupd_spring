      <Button fx:id="btnSave" disable="true" layoutX="54.0" layoutY="158.0" mnemonicParsing="false" onAction="#saveData" prefHeight="25.0" prefWidth="101.0" text="Сохранить" AnchorPane.bottomAnchor="14.0" />
      <Button layoutX="177.0" layoutY="158.0" mnemonicParsing="false" onAction="#actionClose" prefHeight="25.0" prefWidth="101.0" text="Отмена" AnchorPane.bottomAnchor="14.0" />
      <Label layoutX="16.0" layoutY="55.0" text="Номер" textFill="RED" />
      <TextField fx:id="txtNumber" disable="true" layoutX="90.0" layoutY="51.0" prefHeight="25.0" prefWidth="221.0" promptText="впишите номер" />
      <TextArea fx:id="txtName" layoutX="90.0" layoutY="87.0" prefHeight="60.0" prefWidth="221.0" />
   </children>
</AnchorPane>
